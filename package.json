{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:prettier",
    "lint:js": "eslint --ignore-pattern '**/*.cjs'",
