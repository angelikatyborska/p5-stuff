    "@types/p5": "^1.7.6",
    "astro": "^5.10.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "p5": "^1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.21.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^38.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
